@model  IEnumerable<Category>
@{
   
}

<div class="text-center">
    <h1>this Category Page</h1>
</div>
<div class="m-5">
    <table>
        <tr>
            <th>id</th>
            <th>name</th>
            <th>Description</th>
            <th>Image</th>
            <th>RestaurantId</th>
            
        </tr>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Id</td>
                <td>@obj.Name</td>
                <td>@obj.Description</td>
                <td>@obj.ImageUrl</td>
                <td>@obj.RestaurantId</td>
               
            </tr>
        }

    </table>
    
</div>